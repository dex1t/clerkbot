{
  "name": "Clerkbot",
  "description": "a Slack Bot to interact with Intercom for Customer Success",
  "keywords": [
    "ChatBot",
    "Customer Success",
    "Slack",
    "Intercom"
  ],
  "repository": "https://github.com/dex1t/clerkbot",
  "logo": "https://raw.githubusercontent.com/dex1t/clerkbot/master/icon.png",
  "env": {
    "SLACK_BOT_TOKEN": {
      "description": "Bot User OAuth Access Token https://api.slack.com/apps"
    },
    "SLACK_APP_CLIENT_ID": {
      "description": "Slack App Credentials https://api.slack.com/apps"
    },
    "SLACK_APP_CLIENT_SECRET": {
      "description": "Slack App Credentials https://api.slack.com/apps"
    },
    "INTERCOM_ACCESS_TOKEN": {
      "description": "Intercom Access Token https://app.intercom.io/developers/_"
    }
  },
  "addons": [
    "redistogo:nano"
  ]
}
